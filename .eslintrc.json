{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true,
    "jasmine": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2021
  },
  "globals": {
    "Test262Error": true,
    "gc": true,
    "$SOURCE": true,
    "fixture": true,
    "sandbox": true,
    "sinon": true
  },
  "rules": {
    "no-console": [0],
    "no-empty-pattern": [0]
  }
}
